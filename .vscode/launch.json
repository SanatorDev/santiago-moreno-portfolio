{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js: debug dev server",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev"
    },
    {
      "name": "Next.js: debug client (Chrome)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ],
  "compounds": [
    {
      "name": "Next.js: full stack",
      "configurations": [
        "Next.js: debug dev server",
        "Next.js: debug client (Chrome)"
      ]
    }
  ]
}
